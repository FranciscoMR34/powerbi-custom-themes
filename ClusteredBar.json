{
    "visualStyles": {
        "clusteredBarChart": {
            "*": {
                "title": [
                    {
                        "show": true,
                        "textColor": "#000000",
                        "text": "Añadir titulo",
                        "fontSize": 16,
                        "alignment": "left",
                        "fontFamily": "Segoe UI Semibold"
                    }
                ],
                "subTitle": [
                    {
                        "show": true,
                        "text": "Añadir subtitulo",
                        "fontSize": 12,
                        "fontFamily": "Segoe UI",
                        "alignment": "left",
                        "fontColor": {
                            "solid": {
                                "color": "#909288"
                            }
                        }
                    }
                ],
                "divider": [
                    {
                        "show": true,
                        "color": {
                            "solid": {
                                "color": "#C7C8C7"
                            }
                        },
                        "thickness": 1,
                        "ignorePadding": false
                    }
                ],
                "spacing": [
                    {
                        "spaceBelowTitle": 5,
                        "spaceBelowSubTitle": 10,
                        "spaceBelowTitleArea": 5
                    }
                ],
                "padding": [
                    {
                        "top": 15,
                        "bottom": 20,
                        "left": 15,
                        "right": 15
                    }
                ],
                "border": [
                    {
                        "show": true,
                        "color": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "radius": 15
                    }
                ],
                "legend": [
                    {
                        "show": false
                    }
                ]
            },
            "Paged": {
                "categoryAxis": [
                    {
                        "showAxisTitle": false,
                        "fontSize": 14
                    }
                ],
                "valueAxis": [
                    {
                        "show": false,
                        "showAxisTitle": false
                    }
                ],
                "labels": [
                    {
                        "show": true,
                        "showAll": true,
                        "fontSize": 13,
                        "labelDisplayUnits": 1,
                        "labelPrecision": 1
                    }
                ]
            },
            "Custom": {
                "categoryAxis": [
                    {
                        "show": true,
                        "fontSize": 12,
                        "showAxisTitle": false
                    }
                ],
                "valueAxis": [
                    {
                        "show": false,
                        "showAxisTitle": false,
                        "gridlineShow": true,
                        "fontSize": 11,
                        "labelPrecision": 0
                    }
                ],
                "labels": [
                    {
                        "show": true,
                        "showAll": true,
                        "fontSize": 12,
                        "labelDisplayUnits": 1,
                        "labelPrecision": 0
                    }
                ],
                "background": [
                    {
                        "show": true,
                        "color": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "transparency": 0
                    }
                ]
            }
        }
    }
}
