{
    "visualStyles": {
        "clusteredColumnChart": {
            "*": {
                "stylePreset": [
                    {
                        "name": ""
                    }
                ],
                "title": [
                    {
                        "show": true,
                        "textColor": "#000000",
                        "text": "Añadir titulo",
                        "fontSize": 16,
                        "alignment": "left",
                        "fontFamily": "Segoe UI Semibold"
                    }
                ],
                "subTitle": [
                    {
                        "show": true,
                        "text": "Añadir subtitulo",
                        "fontSize": 12,
                        "fontFamily": "Segoe UI",
                        "alignment": "left",
                        "fontColor": {
                            "solid": {
                                "color": "#909288"
                            }
                        }
                    }
                ],
                "divider": [
                    {
                        "show": true,
                        "color": {
                            "solid": {
                                "color": "#C7C8C7"
                            }
                        },
                        "thickness": 1,
                        "ignorePadding": false
                    }
                ],
                "spacing": [
                    {
                        "spaceBelowTitle": 5,
                        "spaceBelowSubTitle": 10,
                        "spaceBelowTitleArea": 5
                    }
                ],
                "padding": [
                    {
                        "top": 15,
                        "bottom": 15,
                        "left": 15,
                        "right": 15
                    }
                ],
                "border": [
                    {
                        "show": true,
                        "color": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "radius": 15
                    }
                ]
            },
            "Executive": {
                "categoryAxis": [
                    {
                        "showAxisTitle": false,
                        "fontSize": 12
                    }
                ],
                "valueAxis": [
                    {
                        "show": false,
                        "showAxisTitle": true
                    }
                ],
                "visualHeader": [
                    {
                        "show": true,
                        "showDrillDownExpandButton": true,
                        "showDrillDownLevelButton": true,
                        "showDrillRoleSelector": true,
                        "showDrillToggleButton": true,
                        "showDrillUpButton": true,
                        "showCommentButton": false,
                        "showCopyVisualImageButton": false,
                        "showFilterRestatementButton": false,
                        "showFocusModeButton": false,
                        "showFollowVisualButton": false,
                        "showOptionsMenu": false,
                        "showPersonalizeVisualButton": false,
                        "showPinButton": false,
                        "showSeeDataLayoutToggleButton": false,
                        "showSetAlertButton": false,
                        "showSmartNarrativeButton": false,
                        "showTooltipButton": false,
                        "showVisualErrorButton": false,
                        "showVisualInformationButton": false,
                        "showVisualWarningButton": false
                    }
                ]
            }
        }
    }
}
