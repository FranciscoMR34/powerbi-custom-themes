{
	"name": "MatrixTemplate",
	"visualStyles": {
		"pivotTable": {
			"*": {
					"stylePreset":[{
                    			"name":"Condensed"
                			}],
					"grid": [{
					"gridVertical": false,
					"gridVerticalColor": { "solid": { "color": "#F5F5F5"}},
					"gridVerticalWeight": 2,
					"gridHorizontal": true,
					"gridHorizontalColor": { "solid": { "color": "#C7C8C7"}},
					"gridHorizontalWeight": 1,
					"rowPadding": 2,
					"outlineColor": { "solid": { "color": "#F5F5F5"}},
					"outlineWeight": 2,
					"textSize": 14,
					"imageHeight": 100
				}],
				"columnHeaders": [{
					"fontColor": { "solid": { "color": "#000000"}},
					"backColor": { "solid": { "color": "#E5E1E6"}},
					"outline": "Frame",
					"autoSizeColumnWidth": true,
					"fontFamily": "Segoe UI semibold",
					"fontSize": 14,
					"alignment": "Left",
					"titleAlignment": "Right",
					"urlIcon": true,
					"wordWrap": true
				}],
				"rowHeaders": [{
					"fontColor": { "solid": { "color": "#000000"}},
					"backColor": { "solid": { "color": "#E5E1E6"}},
					"outline": "Frame",
					"stepped": true,
					"steppedLayoutIndentation": 12,					
					"urlIcon": true,
					"wordWrap": true,
					"fontFamily": "Segoe UI",
					"fontSize": 12,
					"alignment": "Center", 
					"showExpandCollapseButtons":true
				}],
				"values": [{
					"fontColorPrimary": { "solid": { "color": "#000000"}},
					"backColorPrimary": { "solid": { "color": "#FFFFFF"}},
					"fontColorSecondary": { "solid": { "color": "#000000"}},
					"backColorSecondary": { "solid": { "color": "#FFFFFF"}},
					"bandedRowHeaders": true,
					"valuesOnRow": true,
					"outline": "Frame",
					"urlIcon": true,
					"wordWrap": true,
					"fontFamily": "Segoe UI",
					"fontSize": 12
				}],
				"subTotals": [{
					"rowSubtotals": true,
					"columnSubtotals": true,
					"fontColor": { "solid": { "color": "#000000"}},
					"fontFamily": "Segoe UI",
					"backColor": { "solid": { "color": "#FFFFFF"}},
					"fontSize": 14,
					"applyToHeaders": true,
					"rowSubtotalsPosition": "Bottom",
					"perRowLevel": true,
					"perColumnLevel": true					
				}],
				"total": [{
					"fontColor": { "solid": { "color": "#000000"}},
					"fontFamily": "Segoe UI",
					"backColor": { "solid": { "color": "#FFFFFF"}},
					"applyToHeaders": true,
					"fontSize": 14

				}]
			}
		}
	}
}
