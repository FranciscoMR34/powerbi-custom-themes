{
    "visualStyles": {
        "slicer": {
            "*": {
                "header": [
                    {
                        "show": true,
                        "textSize": 12,
                        "fontColor": {
                            "solid": {
                                "color": "#909288"
                            }
                        },
                        "background": {
                            "solid": {
                                "color": "#F5F5F5"
                            }
                        }
                    }
                ],
                "slicertext": [
                    {
                        "fontSize": 11,
                        "fontColor": {
                            "solid": {
                                "color": "#909288"
                            }
                        },
                        "fontFamily": "Segoe"
                    }
                ],
                "selection": [
                    {
                        "fontColor": {
                            "solid": {
                                "color": "#909288"
                            }
                        },
                        "background": {
                            "solid": {
                                "color": "#F5F5F5"
                            }
                        },
                        "bold": true
                    }
                ],
                "inputbox": [
                    {
                        "fontSize": 11,
                        "fontColor": {
                            "solid": {
                                "color": "#909288"
                            }
                        },
                        "background": {
                            "solid": {
                                "color": "#F5F5F5"
                            }
                        },
                        "bordercolor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        }
                    }
                ],
                "slider": [
                    {
                        "color": {
                            "solid": {
                                "color": "#C7C8C7"
                            }
                        },
                        "handleFillColor": {
                            "solid": {
                                "color": "#909288"
                            }
                        },
                        "handleBorderColor": {
                            "solid": {
                                "color": "#909288"
                            }
                        },
                        "show": true
                    }
                ],
                "border": [
                    {
                        "color": {
                            "solid": {
                                "color": "#C7C8C7"
                            }
                        },
                        "width": 1,
                        "radius": 5
                    }
                ],
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#F5F5F5"
                            }
                        },
                        "transparency": 0
                    }
                ]
            }
        }
    }
}
